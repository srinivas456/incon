			<div class="fileupload-preview fileupload-exists thumbnail" style="max-width: 80px; max-height: 100px; line-height: 20px;"></div>
			<div>
			<span class="btn btn-file"><span class="fileupload-new">Select image</span><span class="fileupload-exists">Change</span><%= f.file_field :imagelr, :type => "file" %><!-- <input type="file" /> --></span>
			<a href="#" class="btn fileupload-exists" data-dismiss="fileupload">Remove</a>
		</div>
		</div>

